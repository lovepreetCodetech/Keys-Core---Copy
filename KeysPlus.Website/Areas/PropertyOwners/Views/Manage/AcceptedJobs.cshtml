@model POJobSearchModel
@{
    ViewBag.Title = "Owner | Accepted Jobs";
}
<div data-bind="visible : MainView">
    <div class="ui grid">
        <div class="eight wide column">
            <h2 class="ui header">Maintenance For Your Properties </h2>
            <span class="text-muted">You can manage jobs for your properties here</span>
        </div>
        <div class="middle aligned eight wide column">
            @{
                Html.RenderPartial("_SearchBox");
            }
        </div>
    </div>
    <br />
    <br />
    <div class="highlighted">
        <div class="ui grid">
            <div class="eight wide column">
                @{
                    Html.RenderPartial("_SortInput");
                }
            </div>
        </div>
    </div>
    <br />
    <br />
    <div class="ui grid">
        <div class="sixteen wide column">

        </div>
    </div>
    <table class="ui very basic table">
        <thead>
            <tr>
                <th class="four wide"><h5 class="ui teal header">Property</h5></th>
                <th class="three wide"><h5 class="ui teal header">Contractor</h5></th>
                <th class="two wide"><h5 class="ui teal header">Quote Accepted</h5></th>
                <th class="four wide"><h5 class="ui teal header">Progress</h5></th>
                <th class="three wide"><h5 class="ui teal header">Actions</h5></th>
            </tr>
        </thead>
        <tbody data-bind="foreach : Items">
            <tr>
                <td>
                    <h5 class="ui grey header" data-bind="text : StreetAddress" style="margin-bottom : 1px"></h5>
                    <h5 class="ui grey header" data-bind="text : CitySub" style="margin-top : 0px; margin-bottom : 5px"></h5>
                </td>
                <td>
                    <span data-bind="if : IsDIY"><span>Myself</span></span>
                    <span data-bind="ifnot : IsDIY"><span data-bind="text:ProviderCompanyName"></span></span>
                </td>
                <td>
                    $<span data-bind="numeric: Model.AcceptedQuote() ? Model.AcceptedQuote  : 0"></span>
                </td>

                <td style="vertical-align : middle">
                    <div class="progress" style="margin : auto">
                        <div class="progress-bar small-text" role="progressbar" data-bind="text : Model.PercentDone() + '%', style: { width: Model.PercentDone() + '%', color : Model.PercentDone()== 0 ? '#337ab7' : '' }, attr: { 'aria-valuenow': Model.PercentDone }"></div>
                    </div>
                </td>
                <td style="vertical-align : middle">
                    <button class="ui mini basic teal button" data-bind="click: $parent.ShowDetail"></span> Details</button>
                    <button class="ui mini basic teal button" data-bind="visible: IsDIY, click:$parent.ShowEdit">Edit</button>
                </td>
            </tr>

        </tbody>

    </table>
    <div class="ui divider"></div>
