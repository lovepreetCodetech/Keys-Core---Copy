                                            <div class="row" style="line-height : 25px">
                                                <div class="col-md-4" style="padding-right : 0px;">
                                                    <strong>Listed on:</strong>
                                                </div>
                                                <div class="col-md-8">
                                                    <span data-bind="text:moment(CreatedOn()).format('LL')"></span>
                                                </div>
                                            </div>
                                        </div><!--details column-->
                                        <div class="col-md-4 pull-right" style="padding-right: 30px">
                                            <div class="row">
                                                <!--photos-->
                                                <div data-bind="visible:ImgFiles().length>0">
                                                    <div class="carousel" data-ride="carousel" data-bind="attr :{id : 'detailsCarousel'+ Id()}">
                                                        <div class="carousel-inner" style="margin:auto" data-bind="foreach: ImgFiles">
                                                            <div class="item" data-bind="css:{active: $index() == 0}">
                                                                <img class="img-responsive img-thumbnail" data-bind="attr: {src: Data}"
                                                                     onerror="this.src = '/images/no-prop-img.png'" alt="firstImg">
                                                            </div>
                                                        </div>
                                                        <a class="left carousel-control" data-slide="prev" data-bind="attr : {href : '#' + 'detailsCarousel'+ Id()}, if:ImgFiles().length>1">
                                                            <!-- Controls -->
                                                            <span class="fa fa-2x fa-angle-left" aria-hidden="true"></span>
                                                            <span class="sr-only">Previous</span>
                                                        </a>
                                                        <a class="right carousel-control" data-slide="next" data-bind="attr : {href : '#' + 'detailsCarousel'+ Id()}, if:ImgFiles().length>1">
                                                            <span class="fa fa-2x fa-angle-right" aria-hidden="true"></span>
                                                            <span class="sr-only">Next</span>
                                                        </a>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-10">
                                                    <!--Without Images -->
                                                    <div class="thumbnail" data-bind="visible : ImgFiles().length==0">
                                                        <img src="/images/no-prop-img.png" alt="noPropImg" />
                                                    </div>
                                                </div>
                                            </div><!--Without Images -->
                                        </div><!--photo column-->
                                    </div>
                                    <div class="row" style="padding-top:15px;">
                                        <div class="prop-actions">
                                            <button class="btn btn-success" data-bind="visible: ( @ViewBag.LoginId == ProviderId() ) || (!ProviderId()), click:$parent.editJob"><span class="zmdi zmdi-edit"></span> Edit</button>
                                        </div>
                                    </div>
                                </div><!--panel body + panel heading + col-md-12-->
                            </div>
                        </div><!--Progress + Advertised Job-->
                    </div><!--tab-content + Tabs-->
                </div><!--Job Details-->
            </div><!--panel panel-default-->
        </div>*@
</div>

<div data-bind="visible : EditView" style="display : none">
    <div id="viewJobDetails" style="width : 80%; margin : auto">
        <h5 class="ui header pointer-cursor" data-bind="click: $root.ShowMain"><a class="pointer-cursor"><i class="angle left icon"></i>Back To Previous</a></h5>
        <h3 class="header">Edit Job</h3>
        <div class="ui divider"></div>
        <div class="ui segment">
            <form class="ui form" data-bind="with:SelectedItemCopy">
                <div class="field">
                    <label>Property : </label>
                    <span data-bind="text:StreetAddress"></span>
                    <span data-bind="text:CitySub"></span>
                </div>
                <div class="field">
                    <label>Percentage Done :</label>
                    <div class="ui input">
                        <select class="ui selection dropdown" data-bind="value: Model.PercentDone">
                            <option value="0">0%</option>
                            <option value="10">10%</option>
                            <option value="20">20%</option>
                            <option value="30">30%</option>
                            <option value="40">40%</option>
                            <option value="50">50%</option>
                            <option value="60">60%</option>
                            <option value="70">70%</option>
                            <option value="80">80%</option>
                            <option value="90">90%</option>
                            <option value="100">100%</option>
                        </select>
                    </div>

                </div>
                <div class="field">
                    <label>Description:</label>
                    <div class="ui input">
                        <textarea class="form-control" data-bind="value:Model.JobDescription, valueUpdate:'afterkeydown'" id="jobDescription" maxlength="500" rows="6" cols="70"></textarea>
                    </div>
                </div>
                <div class="field">
                    <label class="control-label clear">Note:</label>
                    <div class="ui input">
                        <textarea class="form-control" data-bind="value:Model.Note, valueUpdate:'afterkeydown'" id="note" maxlength="500" rows="6" cols="70"></textarea>
                    </div>
                </div>
                <div class="field">
                    <label>Please upload your documents here:</label>
                    <div class="ui divider"></div>
                    @Html.Partial("_MediaInput")
                    <p class="text-muted" style="font-size:11px; text-align : left">You may upload up to 5 files. Total size is 5 MB and supported file types are *.jpg, *.jpeg, *.png, *.gif, *.pdf & *.doc*</p>
                </div>
                <div class="field text-center">
                    <button id="saveButton" class="ui basic small teal button" data-bind="click: $root.Edit, enable: IsValid">Save</button>
                    <button class="ui basic small orange button" data-bind="click: $root.ShowMain">Cancel</button>
                </div>

            </form>
        </div>

    </div>
    <br />
</div>

