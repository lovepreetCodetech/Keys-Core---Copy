                    <button id="addJobDIY" class="ui teal button" data-bind="click:$root.AddJob , enable :IsValid" autofocus>Submit</button>
                    <button class="ui red button" id="back-to-tenantRequests" data-bind="click : $root.ShowMain" autofocus>Cancel</button>
                </div>
            </div>
        </div>
    </div>
