                </div>
                <div class=" field">
                    <label>Job Description</label>
                    <textarea rows="5" style="height:123px;" data-bind="value: Model.JobDescription, valueUpdate:'afterkeydown'"></textarea>

                </div>
                <div class="field">
                    <label>Maximum Budget</label>
                    <input type="text" data-bind="value: Model.MaxBudget, valueUpdate:'afterkeydown'">

                </div>
            </div>
