@model POTenantRequestSearchModel
@{
    ViewBag.Title = "Property Owner | Tenant Requests";
}
<div id="mainPage" data-bind="visible : MainView">
    <div class="ui grid">
        <div class="eight wide column">
            <h2 class="ui header">Request By Tenants</h2>
            <span class="text-muted">You can manage tenants' requests here</span>
        </div>
        <div class="middle aligned eight wide column">
            @{
                Html.RenderPartial("_SearchBox");
            }
        </div>
    </div>
    <br />
    <br />
    <br />
    <div class="highlighted">
        <div class="ui grid">
            <div class="eight wide column">
                @{
                    Html.RenderPartial("_SortInput");
                }
            </div>
        </div>
    </div>
    <br />
    <br />
    <div data-bind="if:Items().length <= 0">
        <div class="row">
            <div class="col-md-12">
                <p> You do not have any tenants' requests</p>
            </div>
        </div>
    </div>
    <div class="ui secondary pointing menu">
        <div class="item @(Model.RequestStatus.Equals(PropertyRequestStatus.Submitted) ? "active" : "")">
            @Html.ActionLink("Current", "TenantRequests", new { RequestStatus = PropertyRequestStatus.Submitted })@*</a>*@
        </div>
        <div class="item @(Model.RequestStatus.Equals(PropertyRequestStatus.Accepted) ? "active" : "")">
            @Html.ActionLink("Accepted", "TenantRequests", new { RequestStatus = PropertyRequestStatus.Accepted })@*</a>*@
        </div>
        <div class="item @(Model.RequestStatus.Equals(PropertyRequestStatus.Declined) ? "active item" : "")">
            @Html.ActionLink("Declined", "TenantRequests", new { RequestStatus = PropertyRequestStatus.Declined })@*</a>*@

        </div>
    </div>

    <br />
    <div class="ui two cards" data-bind="foreach: Items, visible : Items().length > 0" >
        <div class="card">
            <div class="content">
                <img class=" mini circular right floated mini ui image" data-bind="attr: {src: TenantProfileFoto}" alt="firstImg" onerror="this.src = '/images/icon-user-default.png'" />
                <div class="header" data-bind="text: TenantName">
                </div>
                <div class="meta">
                    <div data-bind="text : StreetAddress"></div>
                    <div data-bind="text : CitySub"></div>
                </div>
                <div class="description">
                    <span data-bind="text : RequestType" style="line-height:20px"></span>:  <span data-bind="text : Model.RequestMessage" style="line-height:20px" @*style="word-break : break-all"*@></span>
                </div>
                <div class="description">
                    <a class="pointer-cursor" data-bind="click: $root.ShowDetail">Click here for more</a>
                </div>
            </div>
            <div class="extra content">
                <div class="ui two buttons">
                    @if (Model.RequestStatus.Equals(PropertyRequestStatus.Submitted))
                    {
                        <div class="ui basic green button" data-bind="click :  $root.ShowAccept">Accept</div>
                        <div class="ui basic red button" data-bind="click: $root.ShowDecline">Decline</div>
                    }

                </div>
            </div>
        </div>
    </div>

    @{
        Html.RenderPartial("_PagedItems");
    }
</div>
<div data-bind="visible:AcceptView">
    <h3>Accept Tenant Request</h3>
    <hr />
    <a class="btn btn-info btn-sx" data-bind="click : $root.ShowMain"><span class="fa fa-arrow-left"></span> Back</a>
    <br /><br />
    <div class="panel panel-default">
        <div class="panel-body">
            <div class="row" data-bind="with:  SelectedItem">

                <div class="col-md-7">
                    <!--details-->
                    <div class="row" style="line-height : 25px">
                        <div class="col-md-4" style="padding-right : 0px;">
                            <strong>Location:</strong>
                        </div>
                        <div class="col-md-8" style="padding-right : 0px;">
                            <div data-bind="text : StreetAddress"></div>
                            <div data-bind="text : CitySub"></div>
                        </div>
                    </div>
                    <div class="row" style="line-height : 25px">
                        <div class="col-md-4" style="padding-right : 0px;">
                            <strong><span data-bind="text : RequestType" style="line-height:20px"></span>:</strong>
                        </div>
                        <div class="col-md-8" style="padding-right : 0px;">
                            <span data-bind="text : Model.RequestMessage" style="word-break : break-all"></span>
                        </div>
                    </div>
                    <div class="row" style="line-height : 25px">
                        <div class="col-md-4" style="padding-right : 0px;">
                            <strong>Documents:</strong>
                        </div>
                        <div class="col-md-8" data-bind="if: DocFiles().length>0">
                            <div data-bind="foreach: DocFiles()">
                                <ul>
                                    <li>
                                        <a target="_blank" data-bind="attr:{href: Data}, text:OldFileName"></a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-md-8" data-bind="ifnot : DocFiles().length>0">
                            Not Available
                        </div>
                    </div>
                    <div class="row" style="line-height : 25px">
                        <div class="col-md-4" style="padding-right : 0px;">
                            <strong>Requested On:</strong>
                        </div>
                        <div class="col-md-8" style="padding-right : 0px;">
                            <div data-bind="text:moment(moment.utc(moment(CreatedOn()).format('YYYY-MM-DD HH:mm:ss')).toDate()).format('ll')"></div>
                        </div>
                    </div>
                </div><!--details-->
                <div class="col-md-4">
                    <!--photos-->
                    <div class="row">
                        <div class="col-md-12">
                            <div id="carousel-property" class="carousel slide" data-ride="carousel" margin : auto" data-bind="visible : ImgFiles().length>0">
                                <!-- Wrapper for slides -->
                                <div class="carousel-inner" role="listbox" data-bind="foreach: ImgFiles()">
                                    <div class="item" data-bind="css: {active: $index() == 0}">
                                        <img data-bind="attr: {src: Data}" alt="Thumbnail" data-target="#lightbox" style="height:200px">
                                    </div>
                                </div>

                                <!-- Controls -->
                                <a class="left carousel-control" href="#carousel-property" role="button" data-slide="prev">
                                    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                                    <span class="sr-only">Previous</span>
                                </a>
                                <a class="right carousel-control" href="#carousel-property" role="button" data-slide="next">
                                    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                                    <span class="sr-only">Next</span>
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <!--Without Images -->
                        <div class="col-md-12">
                            <div class="thumbnail" data-bind="visible : ImgFiles().length<=0">
                                <img src="/images/no-prop-img.png" alt="noPropImg" />
                            </div>
                        </div>
                    </div><!--Without Images -->
                </div><!--photos-->
                <div class="row">
                    <div class="col-md-12 text-center">
                        <div>
                            <button type="button" id="declineRequestbutton" class="btn btn-primary" data-bind="click: $root.Accept" autofocus>Accept</button>
                            <button type="button" id="declineRequestCancelButton" class="btn btn-danger"
                                    data-bind="click :  $root.ShowMain " autofocus>
                                Cancel
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div><!--panel body-->
    </div><!--panel-->

</div>
<div data-bind="visible:AcceptJobView">
    <h3 class="header">Accept Job Request</h3>
    <div class="ui divider"></div>
    <h5 class="ui teal header pointer-cursor" data-bind="click: $root.ShowMain" style="margin-top: 0px;"><a class="pointer-cursor"><i class="angle left icon"></i>Back To Previous</a></h5>

