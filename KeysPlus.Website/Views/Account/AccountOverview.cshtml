            </ul>*@
            <br />
            <div id="yourInformation" class="ui segment " data-bind="with: Profile">
                <h3 class="ui header" data-bind="text: FullUserName"></h3>
                <div class="ui grid">
                    <div class="eight wide column">
                        <div class="ui relaxed divided list">
                            <div class="item">
                                <div class="ui grid">
                                    <div class="six wide column">
                                        <h4 class="ui grey header">First Name:</h4>
                                    </div>
                                    <div class="eight wide column">
                                        <span data-bind="text: FirstName"></span>

                                    </div>
                                </div>
                            </div>
                            <div class="item">
                                <div class="ui grid">
                                    <div class="six wide column">
                                        <h4 class="ui grey header">Middle Name:</h4>
                                    </div>
                                    <div class="eight wide column">
                                        <span data-bind="if: MiddleName">
                                            <span data-bind="text: MiddleName"></span>
                                        </span>
                                        <span data-bind="ifnot: MiddleName">
                                            Not specified
                                        </span>

                                    </div>
                                </div>
                            </div>
                            <div class="item">
                                <div class="ui grid">
                                    <div class="six wide column">
                                        <h4 class="ui grey header">Last Name:</h4>
                                    </div>
                                    <div class="eight wide column">
                                        <span data-bind="text: LastName"></span>

                                    </div>
                                </div>
                            </div>
                            <div class="item">
                                <div class="ui grid">
                                    <div class="six wide column">
                                        <h4 class="ui grey header">Place Of Birth:</h4>
                                    </div>
                                    <div class="eight wide column">
                                        <span data-bind="country: PlaceOfBirth"></span>

                                    </div>
                                </div>
                            </div>
                            <div class="item">
                                <div class="ui grid">
                                    <div class="six wide column">
                                        <h4 class="ui grey header">Language:</h4>
                                    </div>
                                    <div class="eight wide column">
                                        <span data-bind="text: Language"></span>

                                    </div>
                                </div>
                            </div>
                            <div class="item">
                                <div class="ui grid">
                                    <div class="six wide column">
                                        <h4 class="ui grey header">Phone Number:</h4>
                                    </div>
