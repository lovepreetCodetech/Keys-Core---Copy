    <h2 class="ui header">User Information </h2>
    <hr />
    <div data-bind="visible:MainView">
        <div class="ui pointing menu">
            <a id="test1" class="active item " data-toggle="tab" href="#yourInformation">
                Your Information
            </a>
            <a id="test2" class="item " data-bind=" if : IsServiceSupplier && Company" data-toggle="tab" href="#yourCompany" >
                Your Company
            </a>
            </div>

